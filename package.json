{
  "name": "@nhhsa/transcript-generator",
  "version": "3.0.0-beta.2",
  "private": true,
  "scripts": {
    "eslink": "npm link @es/changelogger",
    "clog": "auto-changelog",
    "postinstall": "npm run eslink"
  },
  "devDependencies": {
    "auto-changelog": "^2.2.1"
  },
  "auto-changelog": {
    "backfillLimit": false,
    "commitLimit": false,
    "handlebarsSetup": "node_modules/@es/changelogger/templates/changelog-template.js",
    "hideCredit": true,
    "includeBranch": [
      "master",
      "merge"
    ],
    "package": true,
    "releaseSummary": true,
    "sortCommits": "date-desc",
    "template": "node_modules/@es/changelogger/templates/changelog-template.hbs",
    "unreleased": true
  },
  "description": "NHHSA transcript-generator or tg",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esoto76/transcript-generator.git"
  },
  "keywords": [
    "nhhsa-transcript-generator",
    "transcript-generator",
    "nhhsa-tg",
    "tg"
  ],
  "author": "Eric Sotomayor <elpasocolorartist@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/esoto76/transcript-generator/issues"
  },
  "homepage": "https://github.com/esoto76/transcript-generator#readme"
}